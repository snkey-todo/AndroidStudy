<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ictuniv.eventbus3_sample">

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.DesignDemo">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.ActivityA">
        </activity>
        <activity android:name=".activity.ActivityB">
        </activity>
        <activity android:name=".activity.ActivityC">
        </activity>
        <activity android:name=".activity.ActivityD">
        </activity>
        <activity android:name=".activity.ActivityE">
        </activity>
        <activity android:name=".SecondActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
    </application>

</manifest>