<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android_email"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android_email.MailDetails"
            android:label="@string/mail_details"></activity>
        <activity
            android:name="com.example.android_email.ReceiveList"
            android:label="@string/receivelist"></activity>
        <activity
            android:name="com.example.android_email.SendMail"
            android:label="@string/send_email"></activity>
        <!-- <activity android:name="mi.email.core.ReadDatabase"></activity> -->
        <!-- <activity android:name="mi.email.core.WriteDatabase"></activity> -->
        <activity
            android:name="com.example.android_email.ReceiveAndSend"
            android:label="@string/mail_details"></activity>
        <activity
            android:name="com.example.android_email.QQMailSuggests"
            android:label="@string/login_suggest"></activity>
    </application>

</manifest>